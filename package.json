{
  "name": "cisco-car-exercise",
  "version": "0.0.0",
  "description": "A solution to demo to Cisco",
  "author": "Robert Stanton",
  "license": "No Licence",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "dev": "concurrently \"npm run build\" \"npm run nodemon\"",
    "start": "NODE_ENV=production node dist/app.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "nodemon": "NODE_ENV=development nodemon dist/app.js",
    "lint": "eslint app.ts",
    "lint:fix": "eslint --fix app.ts",
    "type": "quicktype src/data/*.json -o src/types/models/[name].json --lang ts",
    "seeddb": "npm run build && cross-env NODE=development node ./dist/scripts/seedDatabase"
  },
  "_moduleAliases": {
    "@utils": "dist/utils",
    "@models": "dist/models",
    "@controllers": "dist/controllers",
    "@customTypes": "dist/types",
    "@data": "dist/data"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.2",
    "express": "~4.16.1",
    "express-graphql": "^0.12.0",
    "express-validator": "^6.14.2",
    "graphql": "^16.6.0",
    "helmet": "^6.0.0",
    "jest": "^29.0.3",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "pg": "^8.8.0"
    "nodemailer": "^6.7.8",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pg": "^8.8.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.0.1",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.6",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.0",
    "nodemon": "^2.0.19",
    "quicktype": "^15.0.261",
    "typescript": "^4.8.2"
  }
}

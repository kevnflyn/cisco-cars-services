{
  "name": "cisco-car-exercise",
  "version": "0.0.0",
  "description": "A solution to demo to Cisco",
  "author": "Robert Stanton",
  "license": "No Licence",
  "scripts": {
    "dev": "concurrently \"npm run build\" \"npm run nodemon\"",
    "start": "NODE_ENV=production node dist/app.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "nodemon": "NODE_ENV=development nodemon dist/app.js",
    "lint": "eslint app.ts",
    "lint:fix": "eslint --fix app.ts",
    "type": "quicktype src/data/*.json -o src/types/models/[name].json --lang ts"
  },
  "_moduleAliases": {
    "@utils": "dist/utils",
    "@models": "dist/models",
    "@controllers": "dist/controllers",
    "@customTypes": "dist/types",
    "@data": "dist/data"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.2",
    "express": "~4.16.1",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.0",
    "nodemon": "^2.0.19",
    "quicktype": "^15.0.261",
    "typescript": "^4.8.2"
  }
}
